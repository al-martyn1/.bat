@set OUTROOTPATH_MSVC2017_X86=%~dp0\..\.out\msvc2017\x86
@set OUTROOTPATH_MSVC2017_X64=%~dp0\..\.out\msvc2017\x64
@set OUTROOTPATH_MSVC2019_X86=%~dp0\..\.out\msvc2019\x86
@set OUTROOTPATH_MSVC2019_X64=%~dp0\..\.out\msvc2019\x64
@set OUTROOTPATH_MSVC2022_X86=%~dp0\..\.out\msvc2022\x86
@set OUTROOTPATH_MSVC2022_X64=%~dp0\..\.out\msvc2022\x64

@set OUTROOTPATH_GCC_73_X86=%~dp0\..\.out\GCC 7.3.0 i686-w64-mingw32\win32-x86
@set OUTROOTPATH_GCC_73_X64=%~dp0\..\.out\GCC 7.3.0 i686-w64-mingw32\x64

@set OUTROOTPATHS="%OUTROOTPATH_MSVC2017_X86%" "%OUTROOTPATH_MSVC2017_X64%" ^
                  "%OUTROOTPATH_MSVC2019_X86%" "%OUTROOTPATH_MSVC2019_X64%" ^
                  "%OUTROOTPATH_MSVC2022_X86%" "%OUTROOTPATH_MSVC2022_X64%" ^
                  "%OUTROOTPATH_GCC_73_X86%" "%OUTROOTPATH_GCC_73_X64%"

@FOR %%i IN (%OUTROOTPATHS%) DO @(
        @IF EXIST "%%~i\" @(
            @set OUTROOTPATH=%%~i
            goto OUTROOTPATH_FOUND
)
)

exit /B 1

:OUTROOTPATH_FOUND
exit /b 0